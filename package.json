  "scripts": {
    "dev": "concurrently --kill-others --names \"FRONTEND,BACKEND\" -c \"blue,green\" \"start cmd /k npm run dev:frontend\" \"start cmd /k npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd backend && npm run dev",
    "build:frontend": "next build",
    "start:frontend": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:frontend": "next lint",
    "type-check": "tsc --noEmit",
    "build:backend": "cd backend && npm run build",
    "start:backend": "cd backend && npm run start:prod",
    "setup": "npm install && cd backend && npm install",
